ns"],[784,"Callouts/Admonitions"],[806,"Callouts/Admonitions"],[828,"Callouts/Admonitions"],[850,"Callouts/Admonitions"],[562,"Câmeras de monitoramento nas áreas de tour"],[667,"Campo de busca funcional sem recarregar página; resultados retornam em ≤\n2 s."],[449,"Cancelamento imediato do projeto"],[226,"Canvas Prosposta de Valor"],[585,"Capacidade de aguardar obstáculo ser removido"],[311,"Captação de novos alunos e parcerias"],[281,"Categoria: Acadêmica / InovaçãoProbabilidade: Muito Alta (90%)Impacto: Alto"],[537,"Categoria: Adoção/Experiência do UsuárioProbabilidade: Baixo (10%)Impacto: Moderado"],[319,"Categoria: Comunicação / BrandingProbabilidade: Moderado (50%)Impacto: Alto"],[306,"Categoria: Divulgação / ExtensãoProbabilidade: Moderada (50%)Impacto: Muito Alto"],[269,"Categoria: Estratégica / MarketingProbabilidade: Muito Alta (90%)Impacto: Muito Alto"],[293,"Categoria: Experiência do UsuárioProbabilidade: Muito Alta (90%)Impacto: Moderado"],[355,"Categoria: IA / Experiência do UsuárioProbabilidade: Muito Baixa (10%)Impacto: Moderado"],[518,"Categoria: IA/ConteúdoProbabilidade: Moderado (30%)Impacto: Moderado"],[344,"Categoria: Operacional / LogísticaProbabilidade: Muito Baixa (10%)Impacto: Muito Alto"],[499,"Categoria: Operacional/ConteúdoProbabilidade: Moderado (50%)Impacto: Moderado"],[464,"Categoria: OperacionalProbabilidade: Moderado (50%)Impacto: Alto"],[571,"Categoria: OperacionalProbabilidade: Muito Alto (90%)Impacto: Baixo"],[444,"Categoria: Segurança/LegalProbabilidade: Moderado (50%)Impacto: Muito Alto"],[554,"Categoria: Segurança/OperacionalProbabilidade: Baixo (10%)Impacto: Moderado"],[427,"Categoria: Técnico/Experiência do UsuárioProbabilidade: Alto (70%)Impacto: Muito Alto"],[482,"Categoria: TécnicoProbabilidade: Moderado (30%)Impacto: Alto"],[410,"Categoria: TécnicoProbabilidade: Muito Alta (90%)Impacto: Muito Alto"],[332,"Categoria: Tecnológica / ExperiencialProbabilidade: Baixa (30%)Impacto: Moderado"],[1009,"Cenários de uso do sistema"],[