ise-de-impacto-etico"],[150,"/docs/sprint-1/entendimento-do-negocio"],[151,"/docs/sprint-1/entendimento-do-negocio"],[152,"/docs/sprint-1/entendimento-do-negocio"],[153,"/docs/sprint-1/entendimento-do-negocio"],[154,"/docs/sprint-1/entendimento-do-negocio"],[155,"/docs/sprint-1/entendimento-do-negocio"],[112,"/docs/sprint-1/entendimento-do-negocio/analise-financeira"],[113,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[114,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[115,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[116,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[117,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[118,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[119,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[120,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[121,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[122,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[123,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[124,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[125,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[126,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[127,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[128,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[129,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[130,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[131,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[132,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[133,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[134,"/docs/sprint-1/entendimento-do-negocio/canvas-proposta-de-valor"],[135,"/docs/sprint-1/entendimento-do-negocio/