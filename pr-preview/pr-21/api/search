iança pois não conhece a instituição."},"815":{"id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva-1","page_id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva","type":"heading","tags":[],"url":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva#resumo-da-persona","content":"Resumo da Persona"},"816":{"id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva-2","page_id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva","type":"heading","tags":[],"url":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva#informações-demográficas","content":"Informações Demográficas"},"817":{"id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva-3","page_id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva","type":"heading","tags":[],"url":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva#bio","content":"Bio"},"818":{"id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva-4","page_id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva","type":"heading","tags":[],"url":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva#cenários-de-uso-do-sistema","content":"Cenários de Uso do Sistema"},"819":{"id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva-5","page_id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva","type":"heading","tags":[],"url":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva#dores-e-necessidades","content":"Dores e Necessidades"},"820":{"id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva-6","page_id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva","type":"heading","tags":[],"url":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva#conclusão","content":"Conclusão"},"821":{"id":"/docs/sprint-1/entendimento-do-usuario/personas/persona-executiva-7","page_id":"/docs/sprint-