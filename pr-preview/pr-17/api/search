ologia"],[154,"Verde: Risco baixo - monitoramento básico"],[156,"Vermelho: Risco crítico - ação imediata necessária"],[257,"Versionamento e histórico de mudanças"],[338,"Viabilizar o sucesso do projeto de inovação"],[354,"Visitante"],[217,"Visitantes sem conclusão do tour"]],"type":"string"},"page_id":{"docs":{"1":0,"2":1,"3":2,"4":3,"5":4,"6":5,"7":6,"8":7,"9":14,"10":8,"11":9,"12":10,"13":11,"14":12,"15":13,"16":472,"17":473,"18":474,"19":475,"20":476,"21":477,"22":478,"23":479,"24":480,"25":481,"26":482,"27":483,"28":484,"29":485,"30":486,"31":487,"32":488,"33":489,"34":490,"35":491,"36":492,"37":493,"38":494,"39":495,"40":518,"41":519,"42":520,"43":521,"44":522,"45":523,"46":524,"47":525,"48":526,"49":527,"50":528,"51":529,"52":530,"53":531,"54":532,"55":533,"56":534,"57":535,"58":536,"59":537,"60":538,"61":539,"62":540,"63":541,"64":564,"65":565,"66":566,"67":567,"68":568,"69":569,"70":570,"71":571,"72":572,"73":573,"74":574,"75":575,"76":576,"77":577,"78":578,"79":579,"80":580,"81":581,"82":582,"83":583,"84":584,"85":585,"86":586,"87":587,"88":610,"89":611,"90":612,"91":613,"92":614,"93":615,"94":616,"95":617,"96":618,"97":619,"98":620,"99":621,"100":622,"101":623,"102":624,"103":625,"104":626,"105":627,"106":628,"107":629,"108":630,"109":631,"110":632,"111":633,"112":21,"113":22,"114":15,"115":16,"116":17,"117":18,"118":19,"119":20,"120":23,"121":24,"122":25,"123":26,"124":27,"125":28,"126":29,"127":30,"128":31,"129":32,"130":33,"131":34,"132":35,"133":36,"134":37,"135":38,"136":39,"137":40,"138":41,"139":42,"140":43,"141":44,"142":45,"143":46,"144":47,"145":48,"146":49,"147":50,"148":51,"149":52,"150":53,"151":54,"152":55,"153":56,"154":57,"155":58,"156":59,"157":60,"158":61,"159":62,"160":63,"161":64,"162":65,"163":66,"164":67,"165":68,"166":69,"167":70,"168":71,"169":72,"170":73,"171":74,"172":75,"173":76,"174":77,"175":78,"176":79,"177":80,"178":81,"179":82,"180":83,"181":84,"182":85,"183":86,"184":87,"185":88,"186":89,"187":90,"188":91,"189":92,"190":93,